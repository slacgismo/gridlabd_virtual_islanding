#set dateformat=ISO8601

#set suppress_repeat_messages=1
#set randomseed=5
#set minimum_timestep=30


#include "config.glm"

module influxdb
{
	message_flags "DEBUG";
	default_username "gridlabd";
	default_password "gridlabd";
	default_hostname "localhost";
	default_port 8086;
	default_database "gridlabd";
    synchronous_postdata "TRUE";
}

object database
 {
 	name "primary";
 	logname "log";

 	// single dynamic tag
 	logtag `mytag=test1,guid={GUID},database`;

 	options NEWDB;
 }

 object database
 {
 	name "secondary";
 	logname "log";

 	// multiple dynamic tags
 	logtag "username";
 	logtag `mytag=test2,tag2=abc,hostname,guid={GUID}`;
 }

////////////// MODULES /////////////////////////////////
module residential {
	implicit_enduses REFRIGERATOR|LIGHTS|PLUGS;
//	paneldump_interval 360;
}


module powerflow {
	solver_method NR;
	line_capacitance true;
	convergence_error_handling IGNORE;
}

module generators; // ENABLES SOLAR
module tape{

}

module reliability {
	maximum_event_length 300 s;
	report_event_log TRUE;
}

#if LOCATION==BUR_VT
#define WEATHER=VT-Burlington_International_Ap.tmy3
#endif //LOCATION
#if ${LOCATION}==LA_CA
#define WEATHER=CA-Los_Angeles_Intl_Arpt.tmy3
#endif //LOCATION

#ifdef TIMEZONE
clock {
	timezone ${TIMEZONE};
#ifdef STARTTIME
	starttime "${STARTTIME}";
#endif // STARTTIME
#ifdef STOPTIME
	stoptime "${STOPTIME}";
#endif // STOPTIME
}
#else
#error No timezone specified
#endif // TIMEZONE
class waterheater
{
	loadshape wh_shape;
	char1024 supernode_name;
}

///////////////////////////////////////////////////////
//
// WEATHER
//
///////////////////////////////////////////////////////
#ifexist data/${WEATHER}

module climate;
object climate {
        name weather;
        tmyfile "data/${WEATHER}";
        interpolate QUADRATIC;
}
#else
#error No weather specified.
#endif // exist WEATHER

